[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "levqor-backend"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "levqor-frontend"

[[workflows.workflow]]
name = "levqor-backend"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "gunicorn --workers ${GUNICORN_WORKERS:-2} --threads ${GUNICORN_THREADS:-4} --timeout ${GUNICORN_TIMEOUT:-30} --graceful-timeout 20 --bind 0.0.0.0:8000 --reuse-port --log-level info run:app"
waitForPort = 8000

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "levqor-frontend"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd levqor-site && npm run dev -- -p 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[deployment]
deploymentTarget = "autoscale"
run = ["gunicorn", "--workers", "2", "--threads", "4", "--timeout", "30", "--graceful-timeout", "20", "--bind", "0.0.0.0:5000", "--reuse-port", "--log-level", "info", "run:app"]

[nix]
channel = "stable-25_05"
packages = ["freetype", "glibcLocales"]
